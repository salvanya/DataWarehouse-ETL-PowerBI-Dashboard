              SQLTask:SqlStatementSource="DECLARE @FechaActual DATE = '2015-01-01';&#xA;&#xA;WHILE @FechaActual &lt;= '2025-12-31'&#xA;BEGIN&#xA;  INSERT INTO TIEMPO (FECHA, DIA, MES, ANIO, TRIMESTRE, DIA_SEMANA, NOMBRE_DIA)&#xA;  SELECT&#xA;    @FechaActual AS FECHA,&#xA;    DAY(@FechaActual) AS DIA,&#xA;    MONTH(@FechaActual) AS MES,&#xA;    YEAR(@FechaActual) AS ANIO,&#xA;    DATEPART(QUARTER, @FechaActual) AS TRIMESTRE,&#xA;    DATEPART(WEEKDAY, @FechaActual) AS DIA_SEMANA,&#xA;    DATENAME(WEEKDAY, @FechaActual) AS NOMBRE_DIA;&#xA;  &#xA;  SET @FechaActual = DATEADD(DAY, 1, @FechaActual);&#xA;END;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
